<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>

<fx:root type="HBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
         alignment="CENTER_RIGHT" spacing="19.0">
    <children>
        <HBox fx:id="minHBox" alignment="CENTER" maxHeight="25.0" prefHeight="25.0" prefWidth="16.0">
            <children>
                <ImageView fx:id="minImg" fitHeight="14.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true"/>
            </children>
        </HBox>
        <HBox fx:id="closeHBox" alignment="CENTER" maxHeight="25.0" prefHeight="25.0" prefWidth="16.0">
            <children>
                <ImageView fx:id="closeImg" fitHeight="14.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets right="19.0" />
    </padding>

</fx:root>
